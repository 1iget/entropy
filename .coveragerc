[run]
branch = True
parallel = True
source = entropy
omit =
    entropy/_version.py
    entropy/ientropy.py
    versioneer.py

[report]
exclude_lines =
    pragma: no cover
show_missing = True

[paths]
source =
    entropy
    .tox/*/lib/python*/site-packages/entropy
    .tox/*/site-packages/entropy
